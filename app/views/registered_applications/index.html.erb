<h1>Registered Applications</h1>
<div class="row">
  <div class="col-md-6">
    <% @registered_applications.each do |registered_application| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <p><%= link_to registered_application.name, registered_application %></p>
            <small>
              <%= registered_application.url %>
            </small>
          </h4>
        </div>
      </div>
      <%= link_to "Delete Application", registered_application, method: :delete, class: 'btn btn-success'%>

    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "New Application", new_registered_application_path, class: 'btn btn-success'%>
  </div>
</div>
