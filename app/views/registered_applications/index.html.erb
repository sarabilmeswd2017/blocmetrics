<h2>Registered Applications</h2>
<div class="row">
  <div class="col-md-6">
    <%= @registered_applications.each do |registered_application| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= registered_application.name %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "New Item", new_registered_application_path, class: 'btn btn-success' %> %>
  </div>
</div>
